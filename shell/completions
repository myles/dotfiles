return 

set completions-ignore-case On

for comp in \
	$HOME/.bin/completion/git-completion.bash \
	$HOME/.bin/completion/hg-completion.bash \
	$HOME/.bin/completion/pip-completion.bash \
	$HOME/.bin/completion/git-flow-completion.bash \
	$HOME/.bin/completion/brew-completion.bash \
	$HOME/.bin/completion/todo-completion.bash \
  $HOME/.bin/completion/t-completion.sh
do
	[[ -e $comp ]] && source $comp
done

# Load zsh autocompletion
if [ $SHELL == 'zsh' ]; then
	autoload -Uz compinit
	compinit
fi
