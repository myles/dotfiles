return 

set completions-ignore-case On

for comp in \
	~/.bin/completion/git-completion.bash \
	~/.bin/completion/hg-completion.bash \
	~/.bin/completion/pip-completion.bash
do
	[[ -e $comp ]] && source $comp
done

# Load zsh autocompletion
if [ $SHELL == 'zsh' ]; then
	autoload -Uz compinit
	compinit
fi