[ui]
username = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %> <<%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>>
ssh = ssh -C
ignore = <%= ENV['HOME'] %>/.hgignore

[pager]
pager = LESS='FSRX' less
quite = True
attend = log, glob, slog, diff, qdiff, annotate

[extensions]
hgext.bookmarks = 
color =
hggit =

[bookmarks]
track.current = True